#################################
# General section
#################################

AMQPhost=localhost
AMQPport=5672
AMQPuser=iris
AMQPpasswd=test

language=ru
locale=ru-RU
systemName=cистема

#################################
# Voice synth section
#################################

# Silence mode
silence=0

# Voice engine (google, yandex, ivona)
ttsEngine=google

# API key for Google Speak API V2
googleKey=empty

# API key for Yandex SpeechKit Cloud
yandexKey=empty

# IVONA voice (Tatyana, Maxim)
ivonaVoice=Tatyana

# Access key for IVONA
ivonaAccessKey=empty

# Secret key for IVONA
ivonaSecretKey=empty

#################################
# Zwave section
#################################

zwaveEnabled=1

# Controller port
zwavePort=/dev/ttyUSB0

# Set "true" if you need zwave debug on screen
zwaveDebug=false

# Path for open-zwave "conf" directory
openzwaveCfgPath=./conf/zwave-conf

#################################
# NooLite section
#################################

nooliteEnabled=1

# Is Noolite sender dongle present?
nooliteTXPresent=1

# Is Noolite receiver dongle present?
nooliteRXPresent=1

##################################
# AI
##################################

witaiEnabled=1
witaiKey=6AVGJKDP4EEAAS5IKNCxxxxxxxxx
