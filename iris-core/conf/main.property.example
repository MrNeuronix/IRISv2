language=ru
locale=ru-RU
systemName=cистема

dbHost=localhost
dbName=iris
dbUsername=root
dbPassword=root

#################################
# Audio section
#################################

startThreshold=9
stopThreshold=5

#################################
# REST section
#################################

httpPort=8085
httpHost=localhost
httpUser=admin
httpPassword=changeme

#################################
# Voice synth section
#################################

# Silence mode
silence=0

# Voice engine (google)
ttsEngine=google

#################################
# Zwave section
#################################

zwaveEnabled=1

# Controller port
zwavePort=/dev/ttyUSB0

# Set "true" if you need zwave debug on screen
zwaveDebug=false

# Path for open-zwave "conf" directory
openzwaveCfgPath=./conf/zwave-conf

#################################
# NooLite section
#################################

nooliteEnabled=0

# Controller port
noolitePort=/dev/ttyUSB1

